local function script()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/xysfhub/AdoptMe/refs/heads/main/scripts'))()
end
local function loadui()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/xysfhub/loading/refs/heads/main/loading'))()
end

task.spawn(script)
task.spawn(loadui)
